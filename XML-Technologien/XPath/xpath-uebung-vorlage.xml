<?xml version="1.0" encoding="UTF-8"?>
<xpath-uebung>
  <!--Bitte die Datei mit dem Namen xpath-uebung-<matrikelnummer>.xml umbenennen. Bsp. 'xpath-uebung-00000000.xml'.-->
  <head>
    <autor>
      <!--Ihr Name-->
      <name>Miranda Lipovica</name>
      <!--Ihr Matrikelnummer-->
      <matrikelnummer>11723838</matrikelnummer>
    </autor>
  </head>
  <!--Bitte fügen Sie die XPath-Ausdrücke als Inhalt des jeweiligen Elements <aufgabe> ein.
  + Das Attribut @n entspricht der Nummer der jeweiligen Aufgaben im 'Einführung-XPath.pdf'
  + Sie erhalten dann die Datei zurück, in der im Attribut @korrekt 'ja' oder 'nein' stehen wird.
  + Punkte 2a und 2b zählen jeweils als 0.5 Punkte.
  -->
  <aufgaben>
    <aufgabe korrekt="NaN" n="1">//beam/note</aufgabe>
    <aufgabe korrekt="NaN" n="2a">//*[@tsamp]</aufgabe>
    <aufgabe korrekt="NaN" n="2b">//note[@accid.ges='f']</aufgabe>
    <aufgabe korrekt="NaN" n="3">//*[@oct='3' or @oct='4']</aufgabe>
    <aufgabe korrekt="NaN" n="4">//note[@dur='8']</aufgabe>
    <aufgabe korrekt="NaN" n="5">distinct-values(//*[not(@tstamp)]/name())</aufgabe>
  </aufgaben>
  <punkte>0</punkte>
</xpath-uebung>
